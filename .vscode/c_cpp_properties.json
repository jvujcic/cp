{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/opt/homebrew/bin/g++-12",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "macos-gcc-arm64",
            "compilerArgs": [
                "-std=c++20 ",
                "-Wall ",
                "-Wextra",
                "-Wshadow ",
                "-Wconversion ",
                "-Wfloat-equal ",
                "-Wduplicated-cond ",
                "-Wlogical-op "
            ]
        }
    ],
    "version": 4
}